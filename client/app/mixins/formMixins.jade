
mixin form(formData,formAttrs)
  -formAttrs= formAttrs || {};
  form.form-inline(id=formData.id)&attributes(formAttrs)
    block
mixin input-group()
  .input-group.input-group-lg.col-xs-10.col-xs-offset-1
    block

mixin progress-bar
  .progress.progress-striped.active.col-xs-10(style="height:46px;margin-top:10px")
    .progress-bar(aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:70%;margin-left:-20px;padding:10px 0px;font-size:20px;")
      | Loading ...
